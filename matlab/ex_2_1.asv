clear all; close all;

fs = 5E3;
n = (0:1/fs:6/fs);
t = (0:1E-7:6/fs);

f1 = 2.1E3;
f2 = 2.9E3;
f3 = 7.1E3;
f4 = 7.9E3;

s1 = cos(2*pi*f1*n); c1 = cos(2*pi*f1*t);
s2 = cos(2*pi*f2*n); c2 = cos(2*pi*f2*t);
s3 = cos(2*pi*f3*n); c3 = cos(2*pi*f3*t);
s4 = cos(2*pi*f4*n); c4 = cos(2*pi*f4*t);

figure(1);
set(figure, 'Name', 'Aliasing')
plot(n*1000, s1, 'bx'); plot(t*1000, c1, 'b'); stem(n*1000, c1, 'k');
